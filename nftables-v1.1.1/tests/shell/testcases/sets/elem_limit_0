#!/bin/bash

## requires EXPR

set -e

RULESET="table netdev filter {
	set test123 {
		typeof ip saddr
		limit rate over 1024 kbytes/second
		elements = { 1.2.3.4 limit rate over 1024 kbytes/second }
	}
}"

$NFT -f - <<< $RULESET

(echo "flush ruleset netdev"; $NFT --stateless list ruleset netdev) | $NFT -f -
